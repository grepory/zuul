[Unit]
Description=OpenVPN
After=docker.service
Requires=docker.service

[Service]
EnvironmentFile=/etc/environment
ExecStartPre=-/usr/bin/docker kill %p
ExecStartPre=-/usr/bin/docker rm %p
ExecStartPre=/usr/bin/docker pull quay.io/opsee/zuul
ExecStart=/usr/bin/docker run --privileged=true --name %p -p 1194:1194 quay.io/opsee/zuul server
ExecStop=/usr/bin/docker stop %p

[X-Fleet]
Global=true
